{
    "manifest_version": 2,
    "name": "ConnectTheDucks>",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "/content/main.js"
            ]
        }
    ],
    "background": {
        "scripts": [
            "/background/main.js"
        ]
    },
    "permissions": [
        "activeTab",
        "clipboardRead",
        "clipboardWrite"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "browser_action": {}
}